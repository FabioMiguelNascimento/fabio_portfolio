generator client {
  provider = "prisma-client"
  output   = "../prisma/generated"
}

datasource db {
  provider = "postgresql"
}

model Stamp {
  id String @id @default(uuid()) @db.Uuid
  stampId String
  x Int @unique
  y Int @unique
  scale Float
  rotation Float
  visitorId String?
  createdAt DateTime @default(dbgenerated("now()"))
 }