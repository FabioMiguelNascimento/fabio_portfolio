generator client {
  provider = "prisma-client"
  output   = "../prisma/generated"
}

datasource db {
  provider = "postgresql"
}

enum Types {
  ICON
  EMOJI
  TEXT
}

model Stamp {
  id String @id @default(uuid()) @db.Uuid
  x Int @unique
  y Int @unique
  content String
  type Types
  scale Float
  rotation Float
  visitorId String?
  createdAt DateTime @default(dbgenerated("now()"))
 }